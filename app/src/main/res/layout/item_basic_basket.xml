                    app:layout_constraintStart_toEndOf="@+id/item_9_label"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/item_9_label"
                    app:layout_constraintBottom_toBottomOf="@+id/item_9_label" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- Item 10 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_10_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_9_layout"
