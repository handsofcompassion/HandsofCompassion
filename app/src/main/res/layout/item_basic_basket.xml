                    app:layout_constraintStart_toEndOf="@+id/item_7_label"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/item_7_label"
                    app:layout_constraintBottom_toBottomOf="@+id/item_7_label" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- Item 8 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_8_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_7_layout"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp">

                <TextView
                    android:id="@+id/item_8_label"
                    android:layout_width="wrap_content"
