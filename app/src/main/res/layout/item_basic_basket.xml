                <TextView
                    android:id="@+id/item_10_response"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:textSize="18sp"
                    app:layout_constraintStart_toEndOf="@+id/item_10_label"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/item_10_label"
                    app:layout_constraintBottom_toBottomOf="@+id/item_10_label" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- Item 11 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_11_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_10_layout"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp">

                <TextView
                    android:id="@+id/item_11_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item_11"
