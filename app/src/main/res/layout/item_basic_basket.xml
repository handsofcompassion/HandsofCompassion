                    app:layout_constraintTop_toTopOf="@+id/item_5_label"
                    app:layout_constraintBottom_toBottomOf="@+id/item_5_label" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- Item 6 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_6_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_5_layout"
