            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="20dp">

            <!-- Item 1 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_1_layout"
